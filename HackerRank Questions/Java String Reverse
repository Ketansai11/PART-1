import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        StringBuilder str = new StringBuilder(A);
        str.reverse();
    if(A.equals(str.toString())) System.out.println("Yes");
    else{
        System.out.println("No");
    }
        
    }
}
